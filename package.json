    "description": "Generates new react modules based on mantra design",
    "version": "0.0.1",
    "publisher": "dobdev",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createMantraComponent"
